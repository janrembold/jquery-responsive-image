{
	"name": "jquery-responsive-image",
	"version": "2.0.0",
	"description": "A jQuery responsive image plugin with viewport or container based width matching support",
	"main": "dist/jquery.responsiveImage.js",
	"module": "src/index.ts",
	"typings": "src/index.d.ts",
	"repository": "https://github.com/janrembold/jquery-responsive-image.git",
	"author": "Jan Rembold <janrembold@gmail.com>",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf dist",
		"build": "npm run clean && webpack",
		"build:watch": "npm run clean && webpack --watch",
		"pretest": "npm run build",
		"test": "npm run --silent test:unit",
		"test:unit": "tsc --module commonjs ./test/unit/*.ts && mocha ./test/unit/*.js"
	},
	"devDependencies": {
		"@types/chai": "4.0.6",
		"@types/jquery": "2.0.39",
		"@types/mocha": "2.2.44",
